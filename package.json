{
  "name": "saa1099tracker",
  "displayName": "SAA1099Tracker",
  "version": "2.0.0",
  "license": "MIT",
  "private": true,
  "homepage": "https://github.com/mborik/SAA1099Tracker",
  "description": "SAA1099Tracker is a progressive web chiptune music tracker for Philips SAA 1099 soundchip",
  "author": {
    "name": "Martin Borik",
    "email": "martin@borik.net"
  },
  "repository": {
    "type": "git",
    "url": "git@github.com:mborik/SAA1099Tracker.git"
  },
  "main": "electron/main.js",
  "scripts": {
    "clean": "rm -rf build/*",
    "build": "cross-env NODE_ENV=production webpack --mode production --config ./webpack.config.js",
    "dev": "webpack-dev-server --config ./webpack.config.js --host 0.0.0.0",
    "start": "npm run build && electron . -d",
    "dist": "build -wl --x64 --ia32 --config electron.json",
    "dist-osx": "build -m --x64 --config electron.json"
  },
  "config": {
    "updateUrl": "http://saa1099tracker.borik.net/api/update"
  },
  "dependencies": {
    "bootstrap": "~3.4.1",
    "bootstrap-toggle": "~2.2.2",
    "classnames": "^2.3.2",
    "compare-version": "^0.1.2",
    "core-js": "^3.25.5",
    "digest-stream": "^1.0.1",
    "dom4": "^2.1.5",
    "electron-window-state": "^4.0.1",
    "jquery": "~2.2.4",
    "lodash": "^4.17.21",
    "meow": "^3.7.0",
    "tslib": "~2.3.1",
    "typescript-debounce-decorator": "^0.0.18",
    "workbox-core": "^6.5.4",
    "workbox-precaching": "^6.5.4",
    "workbox-routing": "^6.5.4",
    "workbox-sw": "^6.5.4"
  },
  "devDependencies": {
    "@pmmmwh/react-refresh-webpack-plugin": "^0.5.7",
    "@types/bootstrap": "^3.3.32",
    "@types/bootstrap-toggle": "^2.2.3",
    "@types/jquery": "^2.0.39",
    "@types/lodash": "^4.14.188",
    "@typescript-eslint/eslint-plugin": "^5.39.0",
    "@typescript-eslint/parser": "^5.39.0",
    "autoprefixer": "^10.4.12",
    "copy-webpack-plugin": "^11.0.0",
    "css-loader": "^6.7.1",
    "cssnano": "^5.1.13",
    "electron": "^21.2.1",
    "electron-builder": "^23.6.0",
    "electron-builder-squirrel-windows": "^23.6.0",
    "eslint": "^8.24.0",
    "eslint-plugin-import": "^2.26.0",
    "fork-ts-checker-notifier-webpack-plugin": "^6.0.0",
    "fork-ts-checker-webpack-plugin": "^7.2.13",
    "html-loader": "^4.2.0",
    "html-webpack-plugin": "^5.5.0",
    "less": "^4.1.3",
    "less-loader": "^11.1.0",
    "mini-css-extract-plugin": "^2.6.1",
    "postcss": "^8.4.17",
    "postcss-discard-comments": "^5.1.2",
    "postcss-loader": "^7.0.1",
    "source-map-loader": "^4.0.0",
    "style-loader": "^3.3.1",
    "ts-loader": "^9.4.1",
    "ts-transformer-optimize-const-enum": "^0.1.2",
    "typescript": "^4.8.2",
    "underscore-template-loader": "^1.1.0",
    "webpack": "^5.74.0",
    "webpack-cli": "^4.10.0",
    "webpack-dev-server": "^4.11.1",
    "webpack-notifier": "^1.15.0",
    "workbox-webpack-plugin": "^6.5.4"
  },
  "browserslist": {
    "production": [
      "supports audio-api",
      "supports es6-module",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
