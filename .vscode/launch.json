{
	"version": "0.2.0",
	"configurations": [{
		"name": "Launch",
		"type": "node",
		"request": "launch",
		"program": "${workspaceRoot}\\app\\main.js",
		"stopOnEntry": false,
		"args": [],
		"cwd": "${workspaceRoot}",
		"runtimeExecutable": "${workspaceRoot}\\node_modules\\.bin\\electron.cmd",
		"runtimeArgs": [
			"--enable-logging"
		],
		"env": {
			"NODE_ENV": "development"
		},
		"console": "internalConsole",
		"sourceMaps": false
	}, {
		"name": "Debug",
		"type": "chrome",
		"request": "launch",
		"runtimeExecutable": "${workspaceRoot}\\node_modules\\.bin\\electron.cmd",
		"runtimeArgs": [
			"${workspaceRoot}",
			"--enable-logging",
			"--remote-debugging-port=9222",
			"--dev"
		],
		"sourceMaps": false
	}, {
		"name": "Attach",
		"type": "node",
		"request": "attach",
		"port": 5858,
		"address": "localhost",
		"restart": false,
		"sourceMaps": false,
		"localRoot": "${workspaceRoot}",
		"remoteRoot": null
	}, {
		"name": "Attach to Process",
		"type": "chrome",
		"request": "attach",
		"port": 9222
	}]
}